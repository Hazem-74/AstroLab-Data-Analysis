print(f"the average uncertainty in angle ≈ {df["Δdθ"].mean():.2f} %")
print(f"the average uncertainty in total proper motiom ≈ {df["Δdμ"].mean():.2f} %")
print(f"the average uncertainty in distance ≈ {df["Δd_msc"].mean():.2f} %")


# In[56]:


